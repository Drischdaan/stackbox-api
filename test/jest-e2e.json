{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@stackbox/common/(.*)": "<rootDir>/../libs/common/src/$1",
    "@stackbox/common": "<rootDir>/../libs/common/src",
    "@stackbox/database/(.*)": "<rootDir>/../libs/database/src/$1",
    "@stackbox/database": "<rootDir>/../libs/database/src"
  },
  "setupFilesAfterEnv": [
    "./setup.e2e.ts"
  ]
}